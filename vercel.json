{
  "version": 2,
  "name": "readysetclass",
  "buildCommand": "echo 'No build needed'",
  "outputDirectory": "frontend",
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/dashboard",
      "dest": "/dashboard-v2.html"
    },
    {
      "src": "/v2",
      "dest": "/dashboard-v2.html"
    },
    {
      "src": "/grading.html",
      "dest": "/grading.html"
    },
    {
      "src": "/grading",
      "dest": "/grading.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
